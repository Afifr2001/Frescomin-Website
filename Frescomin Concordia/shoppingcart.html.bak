			<div class="item3">
			
				<img id="mobile" class="mobile-menu" src="./menu.svg" alt="Open Navigation">
			
			</div>
			
		</div>
	</div>

    
    <div class="cartContainer">

        <h3 class= "contTitle"> Shopping cart</h3>
        <table class="cartTable">
            <tr>
                <th>
                    Product
                </th>
                <th>
                    Quantity
                </th>
                <th>
                    Price
                </th>
                <th>
                    Remove
                </th>

            </tr>
            <tr>
                <td>
                    Apples
                </td>
                <td>
                    <button class="quantityButton">-</button>
                    <input class= "quantityBox" type="number" min="1" value="2"/>
                    <button class="quantityButton">+</button>
                </td>
                <td>
                    9.82$
                </td>
                <td>
                    <button class="xBox">X</button>
                </td>
              

            </tr>
            <tr>
                <td>
                    Chicken
                </td>
                <td>
                    <button class="quantityButton">-</button>
                    <input class= "quantityBox" type="number" min="1" value="2"/>
                    <button class="quantityButton">+</button>
                </td>
                <td>
                    10.62$
                </td>
                <td>
                    <button class="xBox">X</button>
                </td>
              

            </tr>
            <tr>
                <td>
                    Tomatoes
                </td>
                <td>
                    <button class="quantityButton">-</button>
                    <input class= "quantityBox" type="number" min="1" value="2"/>
                    <button class="quantityButton">+</button>
                </td>
                <td>
                    8.52$
                </td>
                <td>
                    <button class="xBox">X</button>
                </td>
              

            </tr>
            <tr>
                <td>
                   Milk
                </td>
                <td>
                    <button class="quantityButton">-</button>
                    <input class= "quantityBox" type="number" min="1" value="2"/>
                    <button class="quantityButton">+</button>
                </td>
                <td>
                    3.40$
                </td>
                <td>
                    <button class="xBox">X</button>
                </td>
              

            </tr>
        </table>
        <div class="totalAlign">
            <h6>
                NoProducts: 4
            </h6>
            <h6>
                Subtotal:   32.36
            </h6>
            <h6>
                GST(5%):    1.62  
            </h6>
            <h6>
                QST (10%):  3.23
            </h6>
            <h5>
                Total:     37.21$
            </h5>
        </div>

    </div>


    <section id="bottom" class="info-section">
